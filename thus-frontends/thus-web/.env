# API domain for local development
# Set this to http://localhost:3000 to use the local Node.js backend
VITE_API_DOMAIN=http://localhost:3000

# Login ways (email, phone, github, google, wechat)
# Comma-separated list of enabled login methods
VITE_LOGIN_WAYS=email,phone,github,google,wechat

# Payment priority (stripe, one-off, wxpay+alipay)
VITE_PAYMENT_PRIORITY=

# Sentry
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=
VITE_SENTRY_SOURCEMAPS=01
VITE_SENTRY_ORG=
VITE_SENTRY_PROJECT=

# Umami
VITE_UMAMI_SCRIPT=
VITE_UMAMI_ID=

# Microsoft Clarity
VITE_MS_CLARITY_PROJECT_ID=

# Bugfender
VITE_BUGFENDER_APPKEY=

# OpenPanel
VITE_OPENPANEL_API_URL=
VITE_OPENPANEL_CLIENT_ID=
VITE_OPENPANEL_CLIENT_SECRET=

# PostHog
VITE_POSTHOG_APIHOST=
VITE_POSTHOG_APIKEY=

# Cloudflare Web Analytics
VITE_CF_WEB_ANALYTICS_SRC=
VITE_CF_WEB_ANALYTICS_TOKEN=
VITE_CF_WEB_ANALYTICS_SENDTO=

# Plausible
VITE_PLAUSIBLE_DOMAIN=
VITE_PLAUSIBLE_SRC=

# OpenReplay
VITE_OPENREPLAY_PROJECT_KEY=
VITE_OPENREPLAY_INGEST_POINT=

# GoatCounter
VITE_GOATCOUNTER_DATA=
VITE_GOATCOUNTER_SRC=

# Tinylytics
VITE_TINYLYTICS_SRC=

# Customer Service Link
VITE_CUSTOMER_SERVICE=

# Connectors & Notifications
VITE_CONNECTORS=01
VITE_CONNECT_WECHAT=
VITE_CONNECT_WPS=
VITE_CONNECT_DINGTALK=
VITE_CONNECT_VIKA=
VITE_CONNECT_FEISHU=
VITE_CONNECT_TELEGRAM=
VITE_CONNECT_WHATSAPP=
VITE_CONNECT_LINE=
VITE_CONNECT_TEAMS=
VITE_CONNECT_SLACK=

# Notification priority (wx_gzh, web_push, disable)
VITE_NOTIFICATION_PRIORITY=disable

# If it is set to 01, then don't use sync system even if API_DOMAIN is set
VITE_DONOT_USE_SYNC=

# Phone bound is required or not, if you set it to 01, then phone bound is required
VITE_PHONE_BOUND_REQUIRED=
